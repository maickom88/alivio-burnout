{
  "name": "alivio-burnout",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "alivio-burnout",
  "sourceRoot": "apps/alivio-burnout/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/angular:webpack-browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/alivio-burnout",
        "index": "apps/alivio-burnout/src/index.html",
        "main": "apps/alivio-burnout/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/alivio-burnout/tsconfig.app.json",
        "assets": ["apps/alivio-burnout/src/favicon.ico", "apps/alivio-burnout/src/assets"],
        "styles": ["apps/alivio-burnout/src/styles.scss", "node_modules/bootstrap/dist/css/bootstrap.min.css"],
        "scripts": [],
        "customWebpackConfig": {
          "path": "apps/alivio-burnout/webpack.config.js"
        }
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all",
          "customWebpackConfig": {
            "path": "apps/alivio-burnout/webpack.prod.config.js"
          }
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nx/angular:module-federation-dev-server",
      "options": {
        "port": 4200,
        "publicHost": "http://localhost:4200"
      },
      "configurations": {
        "production": {
          "browserTarget": "alivio-burnout:build:production"
        },
        "development": {
          "browserTarget": "alivio-burnout:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "alivio-burnout:build"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/alivio-burnout/**/*.ts", "apps/alivio-burnout/**/*.html"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/alivio-burnout/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "alivio-burnout:build",
        "port": 4200
      }
    }
  }
}
